import java.util.Scanner;

public class Game_3 {
    public static void main(String[] args) {
        Scanner tanishq = new Scanner(System.in);
        int x = 1;
        while (x!=0) {
            System.out.println("Enter any number if you will enter the correct number, you win ");
            x = tanishq.nextInt();
            if (x == 23) {
                System.out.println("WOW! You Win");
                break;
            }
            else
                System.out.println("oops!You loose");

        }
        tanishq.close();
    }
}
